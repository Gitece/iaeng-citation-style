<?xml version='1.0' encoding='UTF-8'?>
<style xmlns="http://purl.org/net/xbiblio/csl" version="1.0">
  <info>
    <title>Custom No Numbering Full Names</title>
    <id>http://www.zotero.org/styles/custom-no-numbering-full-names</id>
    <link href="http://www.zotero.org/styles/custom-no-numbering-full-names" rel="self"/>
    <author>Generated by ChatGPT</author>
    <category citation_format="author-date"/>
    <updated>2025-05-29T00:00:00+00:00</updated>
  </info>
  <bibliography>
    <layout suffix=".">
      <group>
        <names variable="author">
          <name and_="text" name_as_sort_order="first" initialize_with=""/>
          <label form="long" prefix=", "/>
        </names>
        <text variable="title" prefix=", &quot;" suffix="&quot;"/>
        <text variable="container-title" prefix=", "/>
        <text variable="volume" prefix=", vol. "/>
        <text variable="issue" prefix=", no. "/>
        <text variable="page" prefix=", pp. "/>
        <date variable="issued" prefix=", "/>
        <date-part name="year"/>
      </group>
    </layout>
  </bibliography>
</style>
